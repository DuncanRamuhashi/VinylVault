{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    {
      "src": "./src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./src/app.ts"
    }
  ],
  "functions": {
    "src/app.ts": {
      "maxDuration": 30
    }
  }
}
